{
  "name": "driveline-express-nest-ts-api",
  "version": "0.5.0",
  "description": "DriveLine Application Backend API",
  "author": "OmAr-Kader",
  "private": true,
  "license": "Apache-2.0 license",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:grpc": "nest start --entry-file main-grpc --watch",
    "start:prod": "node dist/main",
    "start:worker:prod": "node dist/main-worker",
    "up:router:dev:build": "npm run build && npm run up:router:dev",
    "up:api:dev:build": "npm run build && npm run up:api:dev",
    "up:router:prod": "docker-compose -f docker-compose.yml -p driveline-prod up -d --build router && find ./dist -mindepth 1 -exec rm -rf {} +",
    "up:router:prod:login": "docker login && npm run up:router:prod",
    "build:up:api:prod": "npm run build && npm run up:api:prod",
    "up:router:prod:build": "npm run build && npm run up:router:prod",
    "up:router:prod:login:build": "npm run build && npm run up:router:prod:login",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "k6 run test/main.js"
  },
  "dependencies": {
    "@clickhouse/client": "^1.16.0",
    "@fastify/compress": "^8.3.1",
    "@fastify/cors": "^11.2.0",
    "@fastify/helmet": "^13.0.2",
    "@fastify/static": "^9.0.0",
    "@grpc/grpc-js": "^1.14.3",
    "@grpc/proto-loader": "^0.8.0",
    "@mongodb-js/zstd": "^7.0.0",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/cache-manager": "^3.1.0",
    "@nestjs/common": "^11.1.11",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.11",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/microservices": "^11.1.11",
    "@nestjs/mongoose": "^11.0.4",
    "@nestjs/platform-fastify": "^11.1.11",
    "@nestjs/platform-socket.io": "^11.1.11",
    "@nestjs/swagger": "^11.2.4",
    "@nestjs/typeorm": "^11.0.0",
    "@nestjs/websockets": "^11.1.11",
    "agenda": "^5.0.0",
    "amqp-connection-manager": "^5.0.0",
    "amqplib": "^0.10.9",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "dotenv": "^17.2.3",
    "fastify": "^5.6.2",
    "ioredis": "^5.9.2",
    "mongoose": "^9.0.1",
    "nodemailer": "^7.0.12",
    "perf_hooks": "^0.0.1",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "resend": "^6.7.0",
    "snappy": "^7.3.3",
    "socket.io": "^4.8.1",
    "source-map-support": "^0.5.21",
    "stripe": "^20.1.2",
    "systeminformation": "^5.30.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@nestjs/cli": "^11.0.14",
    "@nestjs/schematics": "^11.0.9",
    "@types/amqplib": "^0.10.8",
    "@types/ioredis": "^5.0.0",
    "@types/node": "^25.0.6",
    "@types/uuid": "^11.0.0",
    "@types/ws": "^8.18.1",
    "eslint": "^9.39.2",
    "jest": "^30.2.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^17.0.0",
    "prettier": "^3.7.4",
    "ts-loader": "^9.5.4",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.52.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
